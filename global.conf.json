[
  {
    "defaults": {
      "link": {
        "relink": true,
        "create": true,
        "force": true
      }
    }
  },
  {
    "clean": ["~", "~/.config"]
  },
  {
    "link": {
      "~/.vimrc": {
        "path": "dotvim/vimrc"
      },
      "~/.fonts/fontawesome-webfront.ttf": {
        "path": "fontawesome-webfont.ttf"
      },
      "~/.config/qutebrowser/": {
        "path": "qutebrowser/config.py"
      },
      "~/.local/share/qutebrowser/userscripts/": {
        "path": "qutebrowser/userscripts/body_focus"
      }
    }
  },
  {
  }
]
